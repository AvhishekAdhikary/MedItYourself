<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.med_it_yourself"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.med_it_yourself.MediceneShopActivity"
            android:label="@string/i_shop" />
        <activity
            android:name="com.example.med_it_yourself.DoctorActivity"
            android:label="@string/i_doctor" />
        <activity
            android:name="com.example.med_it_yourself.AmbulanceActivity"
            android:label="@string/i_ambulance" />
        <activity
            android:name="com.example.med_it_yourself.ActionPicker"
            android:label="@string/choose1" />
        <activity
            android:name="com.example.med_it_yourself.RegisterActivity"
            android:label="@string/register_main" />
        <activity
            android:name="com.example.med_it_yourself.LoginActivity"
            android:label="@string/login_main" />
        <activity android:name="com.example.med_it_yourself.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>